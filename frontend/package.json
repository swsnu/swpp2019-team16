    "husky": "^3.0.8",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-saga-tester": "^1.0.574"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/proto/*.js",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  }
